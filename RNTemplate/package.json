{
  "name": "rntemplate",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint \"**/*.{ts,tsx}\" --quiet",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" --quiet --max-warnings 0",
    "format": "prettier --write \"**/*.{js, jsx, ts, tsx}\"",
    "format:ci": "prettier --check \"**/*.{js, jsx, ts, tsx}\""
  },
  "dependencies": {
    "expo": "~48.0.18",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@types/react": "~18.0.27",
    "eslint": "^8.42.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "prettier": "^2.8.8",
    "typescript": "^4.9.4"
  },
  "private": true
}
